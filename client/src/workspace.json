{
  "renderer": "geras",
  "trashcan" : true,
  "grid" : {
    "spacing" : 20,
    "length" : 25,
    "color" : "#ccc",
    "snap" : true
  },
  "move" : {
    "scrollbars" : {
      "horizontal" : true,
      "vertical" : true
    },
    "drag" : true,
    "wheel" : false
  },
  "zoom" : {
    "controls" : true,
    "wheel" : true,
    "startScale" : 1.0,
    "maxScale" : 3,
    "minScale" : 0.25,
    "scaleSpeed" : 1.1,
    "pinch" : true
  },
  "toolbox" : {
    "kind": "flyoutToolbox",
    "contents": [
      {
        "kind": "block",
        "type": "set_qubits"
      },
      {
        "kind": "block",
        "type": "hadamard"
      },
      {
        "kind": "block",
        "type": "entangle"
      },
      {
        "kind": "block",
        "type": "pauli_x"
      },
      {
        "kind": "block",
        "type": "pauli_y"
      },
      {
        "kind": "block",
        "type": "pauli_z"
      },
      {
        "kind": "block",
        "type": "rotate_x"
      },
      {
        "kind": "block",
        "type": "rotate_y"
      },
      {
        "kind": "block",
        "type": "rotate_z"
      },
      {
        "kind": "block",
        "type": "reset"
      }
    ]
  }
}